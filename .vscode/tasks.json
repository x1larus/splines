{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc-11 build active file",
			"command": "/usr/bin/gcc-11",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${fileDirname}/main.c",
				"${fileDirname}/splines.c",
				"-o",
				"${fileDirname}/a.out",
				"-lm",
				"-std=c11",
				"-Wall"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/gcc-11"
		}
	]
}